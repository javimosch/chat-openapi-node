{
  "name": "chat-openapi",
  "version": "1.0.0",
  "description": "Chat interface for OpenAPI specifications",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --ignore 'uploads/' --ignore 'imports/'",
    "migrate": "node scripts/migrate-embeddings.js",
    "prune": "node scripts/prune-uploads.js",
    "prune:logs": "node scripts/prune-import-logs.js",
    "prune:all": "npm run prune && npm run prune:logs",
    "files": "node scripts/print-files.js",
    "print-files": "node scripts/print-files.js",
    "db:clean": "node scripts/db-clean.js",
    "db:health": "node scripts/db-health.js",
    "file:remove": "node scripts/remove-file.js"
  },
  "dependencies": {
    "@langchain/openai": "^0.0.14",
    "@pinecone-database/pinecone": "^4.0.0",
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "chromadb": "^1.9.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.3",
    "csv-stringify": "^6.2.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-ejs-layouts": "^2.5.1",
    "js-yaml": "^4.1.0",
    "langfuse": "^3.37.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.91.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
